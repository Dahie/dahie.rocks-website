<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% capture page_title %}{{ title | strip_html | strip_newlines }}{% endcapture %}
<title>{% if page_title != "" %}{{ page_title | escape }} | {{ metadata.title | escape }}{% else %}{{ metadata.title | escape }}: {{ metadata.tagline | escape }}{% endif %}</title>

<meta name="description" content="{{ metadata.description }}" />

<meta name="keywords" content="ukulele, music, ukeboogie, rock, artist, daniel senff">
<meta name="robots" content="index, follow">
<meta name="language" content="English">

<meta property="og:title" content="Dahie | Music">
<meta property="og:site_name" content="{{ metadata.title }}">
<meta property="og:url" content="http://www.dahie.rocks">
<meta property="og:description" content="{{ metadata.description }}">
<meta property="og:type" content="music.musician">
<meta property="og:image" content="/images/bettlekture-carneval-cover.jpg">
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="@dahie" />
<meta name="twitter:creator" content="@dahie" />

<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link href="https://fonts.googleapis.com/css?family=Lato:400,700,900" rel="stylesheet">
<link href="https://fonts.googleapis.com/css?family=Amatic+SC&display=swap" rel="stylesheet">


<link rel="stylesheet" href="{% asset_path css %}" />
<script src="{% asset_path js %}" defer></script>

